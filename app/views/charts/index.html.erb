<p id="notice"></p>

<h1>Charts</h1>
<%= area_chart @formatted_data, xtitle: "Date of Calls", ytitle: "Number of Calls" %>
<table>
  <thead>
    <tr>
      <th>There are <%= @data.count %> Calls</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @data.each do |call| %>
      <tr>
        <td><%= Date.parse(call["date"]) %></td>
        <td><%= call["total_calls"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

