<p id="notice"></p>

<h1>Charts</h1>
<%= line_chart @formatted_data %>

<table>
  <thead>
    <tr>
      <th>There are <%= @data.count %> Calls</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @data.each do |call| %>
      <tr>
        <td><%= Date.parse(call["date"]) %></td>
        <td><%= call["total_calls"] %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

